{
	"name": "bookmark-tags",
	"version": "0.0.0",
	"private": true,
	"description": "Bookmarks is an open source url manager built using nextjs and supabase.",
	"keywords": [
		"next",
		"react",
		"typescript",
		"tailwind",
		"supabase",
		"react-query"
	],
	"bugs": {
		"url": "https://github.com/timelessco/bookmark-tags/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/timelessco/bookmark-tags.git"
	},
	"license": "MIT",
	"author": {
		"name": "Timeless",
		"email": "hello@timeless.co",
		"url": "https://timeless.co/"
	},
	"type": "module",
	"scripts": {
		"prepare": "husky install",
		"dev": "next dev",
		"dev:sourcemap": "SOURCEMAP=true pnpm dev",
		"build": "next build",
		"postbuild": "next-sitemap --config next-sitemap.config.cjs",
		"build:analyze": "ANALYZE=true pnpm build && pnpm start",
		"build:ci": "SKIP_ENV_VALIDATION=true next build",
		"build:sourcemap": "SOURCEMAP=true pnpm build && source-map-explorer .next/static/**/*.js && pnpm start",
		"start": "next start",
		"build:start": "pnpm build && pnpm start",
		"lint": "npm-run-all lint:*",
		"lint:prettier": "prettier --check --cache --cache-location=.prettiercache --ignore-unknown .",
		"lint:eslint": "eslint --cache --color .",
		"lint:types": "tsc --noEmit",
		"lint:css": "stylelint '**/*.css' --cache --color -f verbose",
		"lint:md": "markdownlint **/*.md \".github/**/*.md\"",
		"lint:knip": "knip --production",
		"lint:package-json": "npmPkgJsonLint .",
		"lint:spelling": "cspell --dot --gitignore --cache --no-progress check \"**/*\"",
		"format": "npm-run-all format:*",
		"format:prettier": "prettier --write --cache --cache-location=.prettiercache --list-different --ignore-unknown .",
		"format:eslint": "eslint --cache --color . --fix",
		"format:css": "stylelint '**/*.css' --fix --cache --color -f verbose",
		"format:md": "markdownlint --fix **/*.md \".github/**/*.md\"",
		"populate:dictionary": "del-cli project-words.txt && cspell --words-only --unique --gitignore --cache --dot \"**/*\" | sort --ignore-case >> project-words.txt",
		"release": "release-it",
		"release:dryrun": "release-it --dry-run",
		"test": "echo \"no test specified\" && exit 0",
		"commit": "gacp"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*": [
			"pnpm format:prettier",
			"pnpm lint:spelling"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {
		"@adaptui/react": "1.0.0-alpha.9",
		"@headlessui/react": "^1.6.6",
		"@heroicons/react": "^1.0.6",
		"@supabase/auth-helpers-nextjs": "^0.5.2",
		"@supabase/auth-helpers-react": "^0.3.1",
		"@supabase/supabase-js": "^2.1.3",
		"@tanstack/react-query": "^4.0.10",
		"@tanstack/react-query-devtools": "^4.0.10",
		"allotment": "^1.17.0",
		"ariakit": "^2.0.0-next.41",
		"axios": "^1.0.0",
		"base64-arraybuffer": "^1.0.2",
		"classnames": "^2.3.1",
		"clsx": "^1.2.1",
		"date-fns": "^2.29.3",
		"jsonwebtoken": "^9.0.0",
		"jwt-decode": "^3.1.2",
		"lodash": "^4.17.21",
		"next": "13.1.6",
		"npm": "^9.0.0",
		"react": "18.2.0",
		"react-aria": "^3.22.0",
		"react-avatar": "^5.0.3",
		"react-dom": "18.2.0",
		"react-hook-form": "^7.33.1",
		"react-infinite-scroll-component": "^6.1.0",
		"react-mentions": "^4.4.7",
		"react-select": "^5.4.0",
		"react-spinners": "^0.13.4",
		"react-stately": "^3.20.0",
		"react-toastify": "^9.0.8",
		"slugify": "^1.6.5",
		"tailwind-merge": "^1.6.0",
		"zustand": "^4.0.0"
	},
	"devDependencies": {
		"@commitlint/cli": "17.5.1",
		"@commitlint/config-conventional": "17.4.4",
		"@ianvs/prettier-plugin-sort-imports": "3.7.2",
		"@next/bundle-analyzer": "13.2.4",
		"@next/env": "13.2.4",
		"@next/eslint-plugin-next": "13.2.4",
		"@release-it/conventional-changelog": "5.1.1",
		"@types/jsonwebtoken": "^9.0.1",
		"@types/lodash": "^4.14.191",
		"@types/node": "18.11.18",
		"@types/react": "18.0.27",
		"@types/react-dom": "18.0.10",
		"@types/react-mentions": "^4.1.8",
		"@typescript-eslint/experimental-utils": "5.57.0",
		"autoprefixer": "10.4.14",
		"cspell": "6.31.1",
		"cypress": "^12.5.1",
		"del-cli": "5.0.0",
		"eslint": "8.37.0",
		"eslint-config-canonical": "41.0.1",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-jsonc": "2.7.0",
		"eslint-plugin-tailwindcss": "3.10.1",
		"eslint-plugin-toml": "0.4.0",
		"eslint-plugin-yml": "1.5.0",
		"gacp": "3.0.3",
		"husky": "8.0.3",
		"jsonc-eslint-parser": "2.2.0",
		"knip": "2.2.4",
		"lint-staged": "13.2.0",
		"markdownlint": "0.28.0",
		"markdownlint-cli": "0.33.0",
		"next-bundle-analyzer": "0.6.7",
		"next-sitemap": "4.0.6",
		"npm-package-json-lint": "6.4.0",
		"npm-package-json-lint-config-default": "5.0.0",
		"npm-run-all": "4.1.5",
		"postcss": "8.4.21",
		"prettier": "2.8.7",
		"prettier-plugin-packagejson": "2.4.3",
		"prettier-plugin-tailwindcss": "0.2.6",
		"release-it": "15.9.3",
		"sharp": "0.31.3",
		"source-map-explorer": "2.5.3",
		"stylelint": "15.3.0",
		"stylelint-config-clean-order": "5.0.0",
		"stylelint-config-standard": "31.0.0",
		"tailwindcss": "3.3.0",
		"tilg": "0.1.1",
		"toml-eslint-parser": "0.5.0",
		"typescript": "5.0.2",
		"yaml-eslint-parser": "1.2.0",
		"zod": "3.21.4"
	},
	"packageManager": "pnpm@8.1.0",
	"gacp": {
		"add": false,
		"push": false,
		"emoji": "emoji",
		"editor": false
	}
}
